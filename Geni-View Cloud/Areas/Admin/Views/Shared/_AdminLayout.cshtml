<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Administrator</title>

    @Styles.Render("~/Content/css")
    @Styles.Render("~/Admin/css")
    <link href="~/Content/bootstrap-colorpicker.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/alertify.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/dataTables.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    <div id="wrapper">
        @Html.Partial("_AdminNavbar")

        <div id="sidebar-wrapper">
            @Html.Partial("_AdminSidebar")
        </div>

        <div id="page-content-wrapper">
            <div id="go-top-button"></div>
            @RenderBody()
            <hr />
            <footer>
                <p>Copyright &copy; @DateTime.UtcNow.Year Bytec Group. All Rights Reserved.</p>
            </footer>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/Custom Scripts/dataTables.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/Custom Scripts/jquery.circliful.min.js"></script>
    <script src="~/Scripts/jquery.matchHeight.js" type="text/javascript"></script>
    <script src="~/Scripts/Custom Scripts/bootstrap-colorpicker.min.js"></script>
    <script src="~/Scripts/alertify.min.js" type="text/javascript"></script>
    <!--Reference the SignalR library. -->
    <script src="~/Scripts/jquery.signalR-2.3.0.min.js"></script>
    <script src="~/signalr/hubs"></script>
    <script type="text/javascript" src="~/Scripts/Custom Scripts/notification.js"></script>
    <script type="text/javascript" src="~/Scripts/Custom Scripts/app.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>